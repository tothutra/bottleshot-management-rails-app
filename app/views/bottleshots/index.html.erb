<h2>All of my bottleshots:</h2>
<br>
<ul>
  <% @bottleshots.each do |bottleshot| %>
    <li><%= link_to bottleshot.fullname, user_bottleshot_path(@user, bottleshot) %> <%= "| Delete Button | Edit Button" if bottleshot.status == "not submitted" %></li>
  <% end %>
</ul>
<br>
<form method="get" action="<%=new_user_bottleshot_path(@user)%>">
    <button type="submit">Add A New Bottleshot</button>
</form>
