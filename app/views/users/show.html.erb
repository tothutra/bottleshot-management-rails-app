<h2>My Profile</h2>
<br>
<%= form_for @user do |f| %>
  <%=f.label :name, "Name:" %><br>
  <%=f.text_field :name %><br>
  <br>
  <%=f.label :email, "Email:" %><br> 
  <%=f.text_field :email %><br>
  <br>
  <%=f.label :password, "Password:" %><br>
  <%=f.text_field :password %><br>
  <br>
  <%=f.submit "Update My Profile" %>
<% end %>
